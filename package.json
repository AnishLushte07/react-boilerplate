{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^4.4.1",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "env-cmd": "^10.1.0",
    "lodash": "^4.17.19",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  },
  "scripts": {
    "start": "env-cmd -f .env craco start",
    "build": "env-cmd -f .env craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "pretty": "prettier --write"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^5.6.4",
    "craco-less": "^1.17.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "2.0.5",
    "react-scripts": "3.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.(js|jsx)": [
      "npm run pretty"
    ]
  }
}
